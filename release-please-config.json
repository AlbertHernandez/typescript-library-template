{
  "release-type": "node",
  "include-component-in-tag": false,
  "plugins": [
    {
      "type": "node-workspace"
    }
  ],
  "packages": {
    ".": {
      "package-name": "typescript-library-template"
    }
  },
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
  "changelog-sections": [
    {
      "type": "feat",
      "section": "✨ New Features",
      "hidden": false
    },
    {
      "type": "fix",
      "section": "🐛 Bug Fixes",
      "hidden": false
    },
    {
      "type": "chore",
      "section": "🧹 Chore",
      "hidden": true
    },
    {
      "type": "docs",
      "section": "📚 Documentation",
      "hidden": true
    },
    {
      "type": "perf",
      "section": "🚀 Performance",
      "hidden": false
    },
    {
      "type": "build",
      "section": "🛠️ Build",
      "hidden": false
    },
    {
      "type": "deps",
      "section": "📦 Dependencies",
      "hidden": false
    },
    {
      "type": "ci",
      "section": "🚦 CI",
      "hidden": true
    },
    {
      "type": "refactor",
      "section": "🔄 Refactoring",
      "hidden": false
    },
    {
      "type": "revert",
      "section": "🔙 Reverts",
      "hidden": false
    },
    {
      "type": "style",
      "section": "🎨 Styling",
      "hidden": true
    },
    {
      "type": "test",
      "section": "🧪 Tests",
      "hidden": true
    }
  ]
}
